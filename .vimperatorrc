set titlestring=Firefox
set ds=duckduckgo
"set ds=google
"set ds=bing
"set ds=startpage
"set tabnumbers=true
"set gui=notabs
set gui=tabs
"set scrollbars=noscrollbars
"colorscheme vimPgray
"colorscheme darkness2
"colorscheme seaweed
set toolbars=addons

" Hints!
hi Hint color: white; background-color: #800; border-color: ButtonShadow; font-family: Droid Sans Mono; font-size: 12px; border-width: 0px; border-style: solid; padding 0 1px; font-weight:bold;
hi Hint::after content: attr(number);
hi HintActive color: black; background: #480;
hi HintElem color: black; background-color: #aaa;
hi HintImage opacity: .5;




hi URL color: #555; text-decoration: none; padding: 0 2px; font-family: Droid Sans Mono; 
hi Normal color: #000; background: #fff; overflow-y: hidden; overflow-x: hidden !important; font-family: Droid Sans Mono; 
hi CmdLine color: #000; background: #ccc; font-family: Droid Sans Mono; font-size: 12px; font-weight: bold;
hi StatusLine color: #000; background: #fff; font-family: Droid Sans Mono; font-size: 12px; font-weight: bold; padding: 2px;

" Make Firefox run faster by using JIT

set! javascript.options.jit.chrome=true

" Autocomplete using Firefox Awesomebar subsystem
set complete=l
 
set maxitems=100
set editor=gvim

